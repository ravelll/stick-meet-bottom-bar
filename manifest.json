{
  "manifest_version": 2,
  "name": "stick-meet-bottom-bar",
  "description": "Stick the bottom bar of Google Meet",
  "version": "0.0.1",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["https://meet.google.com/*"],
    "css": ["style.css"]
  }]
}
